<GuiXml>
  <Controls>
    <TopLevelControl
        hidden="true"
        name="ZZDailyWritsUI"
        movable="true"
        mouseEnabled="true"
        allowBringToTop="true"
        clampedToScreen="true"
        >
      <Dimensions x="65" y="130" />

      <OnMouseUp>ZZDailyWrits:SavePos()</OnMouseUp>
      <!--

      <OnUpdate>
        ZZDailyWrits_Update()
      </OnUpdate>

      <OnInitialized>
        ZZDailyWrits_Initialized()
      </OnInitialized>
      -->

      <Controls>
        <Backdrop name="$(parent)BG"
            centerColor="000000"
            edgeColor="202020"
            >
          <AnchorFill />
          <Edge edgeSize="1" />
        </Backdrop>

        <Label name="$(parent)_label_bs" text="bs" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="5"
              offsetY="5"
              relativePoint="TOPLEFT"
              point="TOPLEFT" />
          <Dimensions x="50" y="20" />
        </Label>
        <Label name="$(parent)_status_bs"
            text="??" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="40"
              offsetY="5"
              relativePoint="TOPLEFT" point="TOPLEFT"
              />
          <Dimensions x="50" y="20" />
        </Label>

        <Label name="$(parent)_label_cl" text="cl" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="5"
              offsetY="25"
              relativePoint="TOPLEFT" point="TOPLEFT" />
          <Dimensions x="50" y="20" />
        </Label>
        <Label name="$(parent)_status_cl"
            text="??" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="40"
              offsetY="25"
              relativePoint="TOPLEFT" point="TOPLEFT" />
          <Dimensions x="50" y="20" />
        </Label>

        <Label name="$(parent)_label_ww" text="ww" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="5"
              offsetY="45"
              relativePoint="TOPLEFT" point="TOPLEFT" />
          <Dimensions x="50" y="20" />
        </Label>
        <Label name="$(parent)_status_ww"
            text="??" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="40"
              offsetY="45"
              relativePoint="TOPLEFT" point="TOPLEFT"
              />
          <Dimensions x="50" y="20" />
        </Label>

        <Label name="$(parent)_label_al" text="al" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="5"
              offsetY="65"
              relativePoint="TOPLEFT" point="TOPLEFT" />
          <Dimensions x="50" y="20" />
        </Label>
        <Label name="$(parent)_status_al"
            text="??" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="40"
              offsetY="65"
              relativePoint="TOPLEFT" point="TOPLEFT"
              />
          <Dimensions x="50" y="20" />
        </Label>

        <Label name="$(parent)_label_en" text="en" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="5"
              offsetY="85"
              relativePoint="TOPLEFT" point="TOPLEFT" />
          <Dimensions x="50" y="20" />
        </Label>
        <Label name="$(parent)_status_en"
            text="??" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="40"
              offsetY="85"
              relativePoint="TOPLEFT" point="TOPLEFT"
              />
          <Dimensions x="50" y="20" />
        </Label>

        <Label name="$(parent)_label_pr" text="pr" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="5"
              offsetY="105"
              relativePoint="TOPLEFT" point="TOPLEFT" />
          <Dimensions x="50" y="20" />
        </Label>
        <Label name="$(parent)_status_pr"
            text="??" font="ZoFontGame" color="CCCCCC" >
          <Anchor relativeTo="$(parent)"
              offsetX="40"
              offsetY="105"
              relativePoint="TOPLEFT" point="TOPLEFT"
              />
          <Dimensions x="50" y="20" />
        </Label>


      </Controls>
    </TopLevelControl>
  </Controls>
</GuiXml>
